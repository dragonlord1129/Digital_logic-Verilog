S_0x567c24027730 .scope module, "fourbitcomparator_tb" "fourbitcomparator_tb" 2 1;
 .timescale 0 0;
v0x567c2403c610_0 .var "Data_in_A", 3 0;
v0x567c2403c6d0_0 .var "Data_in_B", 3 0;
v0x567c2403c770_0 .net "flag", 1 0, v0x567c2403c2e0_0;  1 drivers
S_0x567c240278c0 .scope module, "uut" "fourbitcomparator" 2 14, 3 1 0, S_0x567c24027730;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "A";
    .port_info 1 /INPUT 4 "B";
    .port_info 2 /OUTPUT 2 "flag";
v0x567c23feacf0_0 .net "A", 3 0, v0x567c2403c610_0;  1 drivers
v0x567c2403c160_0 .net "B", 3 0, v0x567c2403c6d0_0;  1 drivers
v0x567c2403c240_0 .var "equal", 0 0;
v0x567c2403c2e0_0 .var "flag", 1 0;
v0x567c2403c3c0_0 .var "greater", 0 0;
v0x567c2403c4d0_0 .var "less", 0 0;
E_0x567c24023db0 .event edge, v0x567c2403c3c0_0, v0x567c2403c240_0, v0x567c2403c4d0_0;
E_0x567c240242b0 .event edge, v0x567c23feacf0_0, v0x567c2403c160_0;
    .scope S_0x567c240278c0;
T_0 ;
    %wait E_0x567c240242b0;
    %load/vec4 v0x567c2403c160_0;
    %load/vec4 v0x567c23feacf0_0;
    %cmp/u;
    %jmp/0xz  T_0.0, 5;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x567c2403c4d0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x567c2403c240_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x567c2403c3c0_0, 0, 1;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x567c23feacf0_0;
    %load/vec4 v0x567c2403c160_0;
    %cmp/u;
    %jmp/0xz  T_0.2, 5;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x567c2403c4d0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x567c2403c240_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x567c2403c3c0_0, 0, 1;
    %jmp T_0.3;
T_0.2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x567c2403c4d0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x567c2403c240_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x567c2403c3c0_0, 0, 1;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x567c240278c0;
T_1 ;
    %wait E_0x567c24023db0;
    %load/vec4 v0x567c2403c4d0_0;
    %load/vec4 v0x567c2403c240_0;
    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x567c2403c3c0_0;
    %concat/vec4; draw_concat_vec4
    %dup/vec4;
    %pushi/vec4 1, 0, 3;
    %cmp/u;
    %jmp/1 T_1.0, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 3;
    %cmp/u;
    %jmp/1 T_1.1, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 3;
    %cmp/u;
    %jmp/1 T_1.2, 6;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v0x567c2403c2e0_0, 0, 2;
    %jmp T_1.4;
T_1.0 ;
    %pushi/vec4 1, 0, 2;
    %store/vec4 v0x567c2403c2e0_0, 0, 2;
    %jmp T_1.4;
T_1.1 ;
    %pushi/vec4 2, 0, 2;
    %store/vec4 v0x567c2403c2e0_0, 0, 2;
    %jmp T_1.4;
T_1.2 ;
    %pushi/vec4 3, 0, 2;
    %store/vec4 v0x567c2403c2e0_0, 0, 2;
    %jmp T_1.4;
T_1.4 ;
    %pop/vec4 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x567c24027730;
T_2 ;
    %vpi_call 2 22 "$monitor", "Time: %0d, A: %b, B: %b, Flag: %b", $time, v0x567c2403c610_0, v0x567c2403c6d0_0, v0x567c2403c770_0 {0 0 0};
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x567c2403c610_0, 0, 4;
    %pushi/vec4 12, 0, 4;
    %store/vec4 v0x567c2403c6d0_0, 0, 4;
    %delay 100, 0;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x567c2403c610_0, 0, 4;
    %pushi/vec4 11, 0, 4;
    %store/vec4 v0x567c2403c6d0_0, 0, 4;
    %delay 100, 0;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x567c2403c610_0, 0, 4;
    %pushi/vec4 10, 0, 4;
    %store/vec4 v0x567c2403c6d0_0, 0, 4;
    %delay 100, 0;
    %vpi_call 2 40 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x567c24027730;
T_3 ;
