#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x57821d39bff0 .scope module, "d_flipflop_tb" "d_flipflop_tb" 2 1;
 .timescale 0 0;
v0x57821d3af440_0 .var "clk", 0 0;
v0x57821d3af510_0 .var "d", 0 0;
v0x57821d3af5e0_0 .net "q", 0 0, v0x57821d3af320_0;  1 drivers
S_0x57821d39c180 .scope module, "uut" "d_flipflop" 2 6, 3 1 0, S_0x57821d39bff0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "d";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /OUTPUT 1 "q";
v0x57821d3647b0_0 .net "clk", 0 0, v0x57821d3af440_0;  1 drivers
v0x57821d364ba0_0 .net "d", 0 0, v0x57821d3af510_0;  1 drivers
v0x57821d3af320_0 .var "q", 0 0;
E_0x57821d3634a0 .event posedge, v0x57821d3647b0_0;
    .scope S_0x57821d39c180;
T_0 ;
    %wait E_0x57821d3634a0;
    %load/vec4 v0x57821d364ba0_0;
    %assign/vec4 v0x57821d3af320_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x57821d39bff0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57821d3af440_0, 0, 1;
T_1.0 ;
    %delay 5, 0;
    %load/vec4 v0x57821d3af440_0;
    %inv;
    %store/vec4 v0x57821d3af440_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x57821d39bff0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57821d3af510_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57821d3af510_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57821d3af510_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57821d3af510_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57821d3af510_0, 0, 1;
    %delay 50, 0;
    %vpi_call 2 31 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x57821d39bff0;
T_3 ;
    %vpi_call 2 36 "$monitor", "At time %0t: clk = %b, d = %b, q = %b", $time, v0x57821d3af440_0, v0x57821d3af510_0, v0x57821d3af5e0_0 {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x57821d39bff0;
T_4 ;
    %vpi_call 2 41 "$dumpfile", "dflipflop.vcd" {0 0 0};
    %vpi_call 2 42 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x57821d39bff0 {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "d_flipflop_tb.v";
    "d_flipflop.v";
