:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x57d794fad6d0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x57d794fad860 .scope module, "decoder3_8_tb" "decoder3_8_tb" 3 1;
 .timescale 0 0;
v0x57d794fc0ae0_0 .var "a_in", 2 0;
v0x57d794fc0ba0_0 .net "a_out", 7 0, v0x57d794f75ba0_0;  1 drivers
S_0x57d794f73cf0 .scope module, "uut" "decoder3_8" 3 5, 4 1 0, S_0x57d794fad860;
 .timescale 0 0;
    .port_info 0 /INPUT 3 "a_in";
    .port_info 1 /OUTPUT 8 "a_out";
v0x57d794f757b0_0 .net "a_in", 2 0, v0x57d794fc0ae0_0;  1 drivers
v0x57d794f75ba0_0 .var "a_out", 7 0;
E_0x57d794f74360 .event edge, v0x57d794f757b0_0;
    .scope S_0x57d794f73cf0;
T_0 ;
    %wait E_0x57d794f74360;
    %pushi/vec4 1, 0, 8;
    %ix/getv 4, v0x57d794f757b0_0;
    %shiftl 4;
    %store/vec4 v0x57d794f75ba0_0, 0, 8;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x57d794fad860;
T_1 ;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x57d794fc0ae0_0, 0, 3;
    %vpi_call/w 3 11 "$monitor", "Time = %0t a_in = %b a_out =%b", $time, v0x57d794fc0ae0_0, v0x57d794fc0ba0_0 {0 0 0};
    %delay 10, 0;
    %pushi/vec4 1, 0, 3;
    %store/vec4 v0x57d794fc0ae0_0, 0, 3;
    %delay 10, 0;
    %pushi/vec4 2, 0, 3;
    %store/vec4 v0x57d794fc0ae0_0, 0, 3;
    %delay 10, 0;
    %pushi/vec4 3, 0, 3;
    %store/vec4 v0x57d794fc0ae0_0, 0, 3;
    %delay 10, 0;
    %pushi/vec4 4, 0, 3;
    %store/vec4 v0x57d794fc0ae0_0, 0, 3;
    %delay 10, 0;
    %pushi/vec4 5, 0, 3;
    %store/vec4 v0x57d794fc0ae0_0, 0, 3;
    %delay 10, 0;
    %pushi/vec4 6, 0, 3;
    %store/vec4 v0x57d794fc0ae0_0, 0, 3;
    %delay 10, 0;
    %pushi/vec4 7, 0, 3;
    %store/vec4 v0x57d794fc0ae0_0, 0, 3;
    %end;
    .thread T_1;
    .scope S_0x57d794fad860;
T_2 ;
    %vpi_call/w 3 22 "$dumpfile", "decoder3_8.vcd" {0 0 0};
    %vpi_call/w 3 23 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x57d794fad860 {0 0 0};
    %end;
    .thread T_2;
