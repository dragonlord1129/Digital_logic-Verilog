:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x62e89cf126f0 .scope module, "digitalclock_tb" "digitalclock_tb" 2 1;
 .timescale -9 -12;
v0x62e89cf27b20_0 .var "Clk_1sec", 0 0;
v0x62e89cf27be0_0 .net "hours", 4 0, v0x62e89cf276c0_0;  1 drivers
v0x62e89cf27cb0_0 .net "minutes", 5 0, v0x62e89cf277a0_0;  1 drivers
v0x62e89cf27db0_0 .var "reset", 0 0;
v0x62e89cf27e80_0 .net "seconds", 5 0, v0x62e89cf27950_0;  1 drivers
S_0x62e89cf12880 .scope module, "uut" "digitalclock" 2 12, 3 3 0, S_0x62e89cf126f0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "Clk_1sec";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 6 "seconds";
    .port_info 3 /OUTPUT 6 "minutes";
    .port_info 4 /OUTPUT 5 "hours";
P_0x62e89ced7620 .param/l "HOURS_MAX" 0 3 13, C4<10111>;
P_0x62e89ced7660 .param/l "MINUTES_MAX" 0 3 12, C4<111011>;
P_0x62e89ced76a0 .param/l "SECONDS_MAX" 0 3 11, C4<111011>;
v0x62e89ced5cf0_0 .net "Clk_1sec", 0 0, v0x62e89cf27b20_0;  1 drivers
v0x62e89cf276c0_0 .var "hours", 4 0;
v0x62e89cf277a0_0 .var "minutes", 5 0;
v0x62e89cf27890_0 .net "reset", 0 0, v0x62e89cf27db0_0;  1 drivers
v0x62e89cf27950_0 .var "seconds", 5 0;
E_0x62e89cf0ee70 .event posedge, v0x62e89cf27890_0, v0x62e89ced5cf0_0;
    .scope S_0x62e89cf12880;
T_0 ;
    %wait E_0x62e89cf0ee70;
    %load/vec4 v0x62e89cf27890_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x62e89cf27950_0, 0;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x62e89cf277a0_0, 0;
    %pushi/vec4 0, 0, 5;
    %assign/vec4 v0x62e89cf276c0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x62e89cf27950_0;
    %addi 1, 0, 6;
    %assign/vec4 v0x62e89cf27950_0, 0;
    %load/vec4 v0x62e89cf27950_0;
    %cmpi/e 59, 0, 6;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x62e89cf27950_0, 0;
    %load/vec4 v0x62e89cf277a0_0;
    %addi 1, 0, 6;
    %assign/vec4 v0x62e89cf277a0_0, 0;
    %load/vec4 v0x62e89cf277a0_0;
    %cmpi/e 59, 0, 6;
    %jmp/0xz  T_0.4, 4;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x62e89cf277a0_0, 0;
    %load/vec4 v0x62e89cf276c0_0;
    %pad/u 6;
    %addi 1, 0, 6;
    %pad/u 5;
    %assign/vec4 v0x62e89cf276c0_0, 0;
    %load/vec4 v0x62e89cf276c0_0;
    %cmpi/e 23, 0, 5;
    %jmp/0xz  T_0.6, 4;
    %pushi/vec4 0, 0, 5;
    %assign/vec4 v0x62e89cf276c0_0, 0;
T_0.6 ;
T_0.4 ;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x62e89cf126f0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x62e89cf27b20_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x62e89cf126f0;
T_2 ;
    %delay 500, 0;
    %load/vec4 v0x62e89cf27b20_0;
    %inv;
    %store/vec4 v0x62e89cf27b20_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x62e89cf126f0;
T_3 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x62e89cf27db0_0, 0, 1;
    %delay 1000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x62e89cf27db0_0, 0, 1;
    %delay 86400000, 0;
    %vpi_call 2 33 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x62e89cf126f0;
T_4 ;
    %vpi_call 2 39 "$monitor", "At time %0t: reset = %b, seconds = %d, minutes = %d, hours = %d", $time, v0x62e89cf27db0_0, v0x62e89cf27e80_0, v0x62e89cf27cb0_0, v0x62e89cf27be0_0 {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x62e89cf126f0;
T_5 ;
    %vpi_call 2 45 "$dumpfile", "digital_clock.vcd" {0 0 0};
    %vpi_call 2 46 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x62e89cf126f0 {0 0 0};
    %end;
    .thread T_5;
# The file index is used to find the file name in the following table.
:file_names 4;
